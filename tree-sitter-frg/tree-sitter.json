{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "frg",
      "camelcase": "Frg",
      "title": "Frg",
      "scope": "source.frg",
      "file-types": [
        "frg"
      ],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^frg$",
      "class-name": "TreeSitterFrg"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Frg grammar for tree-sitter",
    "authors": [
      {
        "name": "Shuflduf",
        "email": "shuflduf@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/shuflduf/frg"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}